<?xml version="1.0" encoding="utf-8" ?>
<Window
    x:Class="WinUI3Package.WindowedContentDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:WinUI3Package"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="WindowedContentDialog"
    mc:Ignorable="d">

    <Grid x:Name="ContentDialogGrid">
        <local:ContentDialogContent x:Name="ContentDialogContentControl" Loaded="OnContentLoaded" />
    </Grid>
</Window>
